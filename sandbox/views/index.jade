extends layout

block app
    .container
        h1 User Management
        hr
        .page
        
    script#user_list(type="text/template")
        a.btn.btn-primary(href="#/new") New User
        hr
        table.table.table-striped
            thead
                tr
                    th Username
                    th First Name
                    th Last Name
                    th
            tbody
                <% _.each(users, function(users) { %>
                tr
                    td <%= users.username %>
                    td <%= users.first %>
                    td <%= users.last %>
                    td
                <% }); %> 








